{
  "name": "Don't Like Emotions",
  "version": "0.1",
  "description": "Removes facebook reactions",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["*://*.facebook.com/*"],
      "css": ["style.css"],
      "js": ["jquery.min.js", "removelikeoptions.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "tabs",
    "*://*.facebook.com/*"
  ]
}
