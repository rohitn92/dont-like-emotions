{
  "name": "Don't Like Emotions",
  "version": "0.2",
  "description": "Removes facebook reactions",
  "icons": { "16": "no-emotions-16.png",
           "48": "no-emotions-48.png",
          "128": "no-emotions-128.png" },
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["*://*.facebook.com/*"],
      "css": ["style.css"],
      "js": ["jquery.min.js", "removelikeoptions.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "tabs",
    "*://*.facebook.com/*"
  ]
}
